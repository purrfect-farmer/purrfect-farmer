    "change-case": "^5.4.4",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "cross-env": "^10.0.0",
    "crx": "^5.0.1",
    "crypto-js": "^4.2.0",
    "crypto-random-string": "^5.0.0",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "events": "^3.3.0",
