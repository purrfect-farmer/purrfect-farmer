    "pbf": "^4.0.1",
    "protobufjs": "^7.5.3",
    "qrcode.react": "^4.2.0",
    "radix-ui": "^1.4.2",
    "react": "^19.1.1",
    "react-countdown": "^2.3.6",
    "react-dom": "^19.1.1",
    "react-draggable": "^4.5.0",
    "react-dropzone": "^14.3.8",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
