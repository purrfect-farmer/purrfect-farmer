PM2_APP_NAME="purrfect-fly"
NODE_ENV="production"
PORT=3000

APP_NAME="Purrfect Fly"
JWT_SECRET_KEY=""

TELEGRAM_BOT_TOKEN=""

# Group configuration - Use -100 prefix for group chat ID
TELEGRAM_CHAT_ID="-100{CHAT_ID}"
TELEGRAM_ANNOUNCEMENT_THREAD_ID=""
TELEGRAM_ERROR_THREAD_ID=""
TELEGRAM_FARMING_THREAD_ID=""

# Group messages configuration
DISPLAY_ACCOUNT_TITLE=false
DISABLE_TELEGRAM_MESSAGES=false

# Captcha Solver configuration (2Captcha / CaptchaAi / SolveCaptcha / CaptchaSonic / NoCaptchaAI)
CAPTCHA_PROVIDER="2captcha" # Options: "2captcha", "captchaai", "solvecaptcha", "captchasonic", "nocaptchaai"
CAPTCHA_API_KEY=""

# Proxy configuration (Webshare)
PROXY_ENABLED=false
PROXY_API_KEY=""
PROXY_PAGE=1
PROXY_PAGE_SIZE=100

# Seeker configuration
SEEKER_ENABLED=false
SEEKER_SERVER=""
SEEKER_KEY=""

# Cron
CRON_ENABLED=true
CRON_MODE="sequential"

# Startup
STARTUP_SEND_SERVER_ADDRESS=true

# Auto start farmers
AUTO_START_FARMER=false

# Minimum farmer rating to run (0-5)
MINIMUM_FARMER_RATING=0

# Per farmer configuration
# FARMER_EXAMPLE_ENABLED=true
# FARMER_EXAMPLE_LINK="https://t.me/example_bot/example?start=1234567890"
# FARMER_EXAMPLE_THREAD_ID=""