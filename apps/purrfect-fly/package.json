{
  "type": "module",
  "name": "purrfect-fly",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "fly": "./fly"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "fly": "node fly",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:migrate:refresh": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/sensible": "^6.0.4",
    "@keyv/sqlite": "^4.0.8",
    "@purrfect/shared": "workspace:*",
    "axios": "^1.13.3",
    "base64url": "^3.0.1",
    "bcryptjs": "^3.0.3",
    "chalk": "^5.6.2",
    "close-with-grace": "^2.4.0",
    "commander": "^14.0.2",
    "cron": "^4.4.0",
    "croner": "^9.1.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.2",
    "fastify-cli": "^7.4.1",
    "fastify-plugin": "^5.1.0",
    "globby": "^14.1.0",
    "grammy": "^1.39.3",
    "hpagent": "^1.2.0",
    "http-cookie-agent": "^7.0.3",
    "inquirer": "^12.11.1",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.3",
    "keyv": "^5.6.0",
    "mariadb": "^3.4.5",
    "pako": "^2.1.0",
    "seedrandom": "^3.0.5",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.5",
    "sqlite3": "^5.1.7",
    "telegram": "^2.26.22",
    "tough-cookie": "^5.1.2",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^11.1.0"
  }
}
