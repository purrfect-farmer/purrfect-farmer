    "build-ext:notgram-main": "VITE_ENTRY=notgram-main vite build",
    "bundle-ext": "node bundle-extension.js",
    "clean": "rimraf dist dist-bridge dist-extension",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/icon.png",
    "lint": "eslint .",
    "preview": "vite preview"
