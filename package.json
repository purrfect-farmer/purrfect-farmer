  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all clean build-extension build-bridge",
    "build-pwa": "VITE_PWA=true npm-run-all build:index",
    "build-bridge": "VITE_BRIDGE=true npm-run-all build-ext:* bundle-ext",
    "build-extension": "npm-run-all build:index build-ext:* bundle-ext",
    "build:index": "VITE_ENTRY=index vite build",
    "build-ext:chrome-service-worker": "VITE_ENTRY=chrome-service-worker vite build",
